{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 5,
  "default_container_padding": 5,
  "border": true,
  "border_width": 1,
  "border_offset": 0,
  "theme": {
    "palette": "Base16",
    "name": "Ashes",
    "unfocused_border": "Base03",
    "bar_accent": "Base0D"
  },
  "stackbar": {
    "height": 24,
    "mode": "OnStack",
    "tabs": {
      "width": 300
    }
  },
  "mouse_follows_focus": false,
  "monitors": [
    {
      "workspaces": [
        {
          "name": "一",
          "layout": "BSP"
        },
        {
          "name": "二",
          "layout": "BSP"
        },
        {
          "name": "三",
          "layout": "BSP"
        },
        {
          "name": "四",
          "layout": "BSP"
        },
        {
          "name": "五",
          "layout": "BSP"
        },
        {
          "name": "六",
          "layout": "BSP"
        },
        {
          "name": "七",
          "layout": "BSP"
        },
        {
          "name": "八",
          "layout": "BSP"
        },
        {
          "name": "九",
          "layout": "BSP"
        },
        {
          "name": "十",
          "layout": "BSP"
        }
      ]
    }
  ]
}
